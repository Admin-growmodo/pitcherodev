      ],
      "settings": {
        "padding_top": 80,
        "padding_top_mobile": 20,
        "padding_bottom": 80,
        "padding_bottom_mobile": 20,
        "background": "",
        "top_padding": true,
        "bottom_padding": true,
        "title_left": true,
        "title": "Player Comparisons",
        "text": "<p>Coaches can compare players within a session using player split. This function displays all metric charts with values for each selected player in the comparison.<\/p>",
        "text_color": "#000000",
        "media_position": "flex-row"
      }
    },
