                        </g>
                        <defs>
                        <clipPath id="clip0_8973_38407">
                        <rect width="24" height="27.4286" fill="white" transform="translate(0.110352 0.710938)"/>
                        </clipPath>
                        </defs>
                        </svg>

                        <strong> Book an online demo</strong>
                      </a>
                  </div>
                  <div class="buttons-container">
                    <a
                      href="#register-section"
                      class="Button Button--contact Button--sale md:h-15 md:!text-base md:!w-auto"
                    >
                      Contact Sales
                    </a>
                    {% if section.settings.download-link != blank %}
                      <a
                        download
                        target="_blank"
                        href="{{ section.settings.download-link }}"
                        class="Button--download md:!text-base btnBorder"
                      >
                        Download Brochure
                      </a>
                    {% endif %}
                  </div>
                </div>
                {% else %}
                  <a
                    href="{{ section.settings.checkout_process_page.url }}"
                    class="Button Button--sale !uppercase md-down:hidden md:h-15 md:!text-base md:!w-auto"
                  >
                    {{ "products.product.pre_order" | t }}
                  </a>
                {% endif %}

                {% if product.metafields.membership.gift.value != blank %}
                  <a
                    href="{{ section.settings.checkout_process_page_gift.url | default: section.settings.checkout_process_page.url }}"
                    class="Button Button--tertiary !uppercase md:h-15 md:!text-base md:!w-auto md:ml-7.5"
                  >
                    {{ "products.product.buy_gift" | t }}
                  </a>
                {% endif %}
              </div>
          {% endif %}
        {% else %}
          {% if section.settings.button != blank %}
            <a
              href="{{ section.settings.button_url }}"
              class="Button Button--primary Button--large !uppercase mt-7 md:!w-auto"
              style="
                color: {{ section.settings.button_color | default: '' }};
                background-color: {{ section.settings.button_background_color | default: '' }};
                border-color: {{ section.settings.button_background_color | default: '' }};
              "
            >
              {{ section.settings.button }}
            </a>
          {% else %}
            {% form "product", product %}
              <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">

              <button type="submit" class="Button Button--primary Button--large !uppercase mt-7 md:!w-auto">
                {{ "products.product.add_to_cart" | t }}
              </button>
            {% endform %}
          {% endif %}
        {% endif %}

        

        {% if product.metafields.meta.text != blank %}
          <div class="text-16 font-bold text-neutral-500 mt-6 md-down:text-center md:text-inherit">
            {{ product.metafields.meta.text }}
          </div>
        {% endif %}
      </div>
    </div>
  </div>
</section>
{%  render 'player-modal', 
youth_first_row_heading: section.settings.youth_first_row_heading ,
youth_first_column_heading: section.settings.youth_first_column_heading, 
youth_table_data_first_row: section.settings.youth_table_data_first_row ,
youth_table_data_second_row: section.settings.youth_table_data_second_row,

adult_first_row_heading: section.settings.adult_first_row_heading ,
adult_first_column_heading: section.settings.adult_first_column_heading, 
adult_table_data_first_row: section.settings.adult_table_data_first_row ,
adult_table_data_second_row: section.settings.adult_table_data_second_row,


%}

{% schema %}
{
  "name": "Product",
  "settings": [
    {
      "type": "header",
      "content": "Checkout process"
    },
    {
      "type": "color",
      "id": "title_color",
      "label": "Title Color",
      "default": "#000"
    },
    {
      "type": "checkbox",
      "id": "enable_checkout_process",
      "label": "Enable",
      "default": false
    },
    {
      "type": "text",
      "id": "size_guide_label",
      "label": "Size guide label",
      "default": "Size Guide",
      "info": "work only for GPS PLAYER BUNDLE"
    },
    {
      "type": "header",
      "content": "Youth content"
    },
    {
      "type": "textarea",
      "id": "youth_first_row_heading",
      "label": "First row heading",
      "default": "Heading 1,Heading 2,Heading 3",
      "info": "Limit 3"
    },
    {
      "type": "textarea",
      "id": "youth_first_column_heading",
      "label": "First column heading",
      "default": "Heading 4, Heading 5",
      "info": "Limit 2 Data"
    },
    {
      "type": "textarea",
      "id": "youth_table_data_first_row",
      "label": "First Youth Data - Chest (INCH)",
      "default": "value 1,value 2,value 3",
      "info": "Limit 3 Data"
    },
    {
      "type": "textarea",
      "id": "youth_table_data_second_row",
      "label": "Second Youth Data - Chest (CM)",
      "default": "value 4,value 5,value 6",
      "info": "Limit 3 Data"
    },
    {
      "type": "header",
      "content": "Adult content"
    },
    {
      "type": "textarea",
      "id": "adult_first_row_heading",
      "label": "Adult first row heading (limit 3 data)",
      "default": "Heading 1,Heading 2,Heading 3,Heading 4,Heading 5",
      "info": "Limit 5 Data"
    },
    {
      "type": "textarea",
      "id": "adult_first_column_heading",
      "label": "First column heading",
      "default": "Heading 6, Heading 7",
      "info": "Limit 2 Data"
    },
    {
      "type": "textarea",
      "id": "adult_table_data_first_row",
      "label": "First Adult Data",
      "default": "value 1,value 2,value 3,value 4,value 5",
      "info": "Limit 5 Data"
    },
    {
      "type": "textarea",
      "id": "adult_table_data_second_row",
      "label": "Second Adult Data",
      "default": "value 6,value 7,value 8,value 9,value 10",
      "info": "Limit 5 Data"
    },
    {
      "type": "page",
      "id": "checkout_process_page",
      "label": "Page for Pre-order"
    },
    {
      "type": "page",
      "id": "checkout_process_page_gift",
      "label": "Page for Gift"
    },
    {
      "type": "header",
      "content": "Button"
    },
    {
      "type": "text",
      "id": "button",
      "label": "Text",
      "default": "Register your interest"
    },
    {
      "type": "url",
      "id": "button_url",
      "label": "Link"
    },
    {
      "type": "color",
      "id": "button_color",
      "label": "Color"
    },
    {
      "type": "color",
      "id": "button_background_color",
      "label": "Background color"
    },
    {
      "type": "html",
      "id": "trustpilot-text",
      "label": "Trustpilot text",
      "info": "work only for GPS TEAM BUNDLE",
      "default": "TrustScore <strong>4.5 I </strong><u><strong>65</strong> reviews</u>"
    },
    {
      "type": "url",
      "id": "trustpilot",
      "label": "Trustpilot url",
      "info": "work only for GPS TEAM BUNDLE"
    },
    {
      "type": "url",
      "id": "custom-url",
      "label": "Book link",
      "info": "work only for GPS TEAM BUNDLE"
    },
    {
      "type": "url",
      "id": "download-link",
      "label": "Brochure",
      "info": "work only for GPS TEAM BUNDLE"
    }
  ],
  "blocks": [
    {
      "type": "html",
      "name": "Logos",
      "settings": [
        {
          "type": "image_picker",
          "id": "logo",
          "label": "Logo"
        },
        {
          "type": "text",
          "id": "logo-text",
          "label": "Text"
        }
      ]
    },
    {
      "type": "review",
      "name": "Review",
      "limit": 1,
      "settings": [
        {
          "type": "textarea",
          "id": "text_review",
          "label": "Text review"
        },
        {
          "type": "image_picker",
          "id": "image_reviews",
          "label": "Image"
        },
        {
          "type": "url",
          "id": "review_url",
          "label": "Review link"
        }
      ]
    }
  ]
}
{% endschema %}
